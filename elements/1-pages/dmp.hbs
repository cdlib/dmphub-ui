<div class="t-step">
  {{ render '@header' }}
  <main>
    {{> @heading }}
    {{> @intro }}
    {{ render '@required' }}
    <div class="t-step__fields">
      {{ render '@summary' }}
      {{ render '@language' }}
      {{ render '@ethical' }}
      {{ render '@ethicsreport' }}
      {{ render '@primary' }}
      <h3>Other Staff</h3>
      {{ render '@other' }}
    </div>
    {{ render '@progress' }}
  </main>
  {{ render '@footer' }}
</div>
