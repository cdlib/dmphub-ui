<div class="t-step">
  {{ render '@header' }}
  <main>
    {{> @heading }}
    {{> @intro }}
    {{ render '@required' }}
    <div class="t-step__fields">
      {{ render '@title' }}
      {{ render '@abstract' }}
      {{ render '@startdate' }}
      {{ render '@enddate' }}
      {{ render '@funder' }}
      {{ render '@fundingstatus' }}
    </div>
    {{ render '@progress' }}
  </main>
  {{ render '@footer' }}
</div>
